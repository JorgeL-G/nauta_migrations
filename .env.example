# URL de conexión a MongoDB
# Formato: mongodb://[usuario:contraseña@]host[:puerto]/[base_de_datos]
MONGODB_URL=mongodb://localhost:27017/nauta_db

# Nombre de la base de datos (opcional, se puede especificar en la URL)
MONGODB_DATABASE=nauta_db

# Directorio donde se almacenan las migraciones
MIGRATIONS_DIR=migrations
